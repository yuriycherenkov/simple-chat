{
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "name": "webpack-4",
  "version": "0.0.2",
  "description": "Webpack 4",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        },
        "@babel/preset-react"
      ]
    ],
    "ignore": [
      "node_modules"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/register": "^7.0.0-beta.49",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.2",
    "@types/prop-types": "^15.5.4",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.5",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux-actions": "^2.3.0",
    "@types/redux-thunk": "^2.1.0",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "8.0.0-beta.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "dynamic-cdn-webpack-plugin": "^4.0.0-rc.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "jest": "^23.6.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^23.10.4",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^20.0.0",
    "typescript-styled-plugin": "^0.12.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "helmet": "^3.12.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "http-errors": "~1.6.2",
    "husky": "^0.14.3",
    "immutable": "^3.8.2",
    "module-to-cdn": "^3.1.2",
    "morgan": "~1.9.0",
    "nodemon": "^1.17.5",
    "pg": "^7.4.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-toggle": "^4.0.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0",
    "sticky-session": "^1.1.2",
    "style-loader": "^0.20.3",
    "synchronous-promise": "^2.0.4",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typesafe-actions": "^2.0.4",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack-merge": "^4.1.3"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/jest_config/tsconfig.test.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest_config/setupTests.js",
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.(jsx?|tsx?)$",
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon ./server/server_dev --config nodemon.json",
    "build": "webpack --mode production",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts*'",
    "lint-fix": "tslint --fix -p tsconfig.json 'src/**/*.ts*'",
    "eslint": "eslint --quiet 'src/**/*.js*'",
    "test": "npx jest --watch",
    "test:ci": "npx jest",
    "precommit": "npm run lint",
    "db:migrate": "sequelize db:migrate"
  }
}
