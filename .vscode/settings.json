{
  "workbench.colorTheme": "Monokai",
  "editor.fontSize": 14,
  "editor.tabCompletion": true,
  "editor.detectIndentation": false,
  "window.zoomLevel": 0,
  "editor.action.copyLinesDownAction": "ctrl+shift+d",
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "off",
  "eslint.autoFixOnSave": true,
  "tslint.autoFixOnSave": true,
  "explorer.confirmDragAndDrop": false,
  "gitlens.advanced.messages": {
      "suppressShowKeyBindingsNotice": true
  },
  "editor.tabSize": 2,
  "cSpell.userWords": [
    "accountlink",
    "activedescendant",
    "cengage",
    "component",
    "devtool",
    "devtools",
    "downitem",
    "drop",
    "editability",
    "eslint",
    "extension",
    "flowtype",
    "haspopup",
    "helplink",
    "io",
    "iporps",
    "labelledby",
    "listbox",
    "mootools",
    "myaccount",
    "mysettings",
    "pathinfo",
    "pckg",
    "plugin",
    "precommit",
    "ptions",
    "quotemark",
    "redux",
    "reselect",
    "signout",
    "spacebar",
    "uglifyjs",
    "unmount",
    "visuallyhidden",
    "webpack",
    "will"
  ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
  ],
  "javascript.updateImportsOnFileMove.enabled": "always"
}